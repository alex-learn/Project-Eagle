function more_days_scroller(){
	if(!is_xui){
		$(window).unbind("scroll").scroll(function() {
		   if($(window).scrollTop() + $(window).height() == $(document).height()) {
		       $.get( "<%= @app.get_url('more_days',{:last=>@app.d[6]}) %>",function(data){
					$('#more_days').before(data);
				})
		   }
		});
	}else{
		x$('a').each(function(element, index, xui) {
			
			if(x$(this).attr("no_script")!="true"){
				x$(element).attr("onclick","get_page('"+x$(this).attr("href")+"')").attr("href","#"+x$(this).attr("href")).attr("no_script","true");
			}
		    
		});
	}
}

</script>
