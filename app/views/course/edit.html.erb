	function add_manager(){
		var manager_email = $('#new_manager_input').val();
		var manager_type = $('#new_manager_type').val();
		$.post("/course/save/<%= @course.id.to_s %>",{email:manager_email,type:manager_type},function(data){
			
		})
	}
	function do_form_action(){
		var dat = {"save":current_save};
		var course = "<%= @course.id.to_s %>";
		if(course==""){
			course = "new";
		}
		if($(current_save).attr("action") == 'save' || $(current_save).attr("action") == 'settings'){
			
			$(current_save+" .saveable").each(function(){
				dat[$(this).attr("name")] = $(this).val();
			})
			$.post("/course/save/"+course,dat,function(data){
				window.location = "/course/edit/"+data;
			})
		}else if($(current_save).attr("action") == 'deals'){
			
		}

	}
var current_save = "#info";
$(document).ready(function(){
	
	$('li.browser').click(function(){
		$(current_tab).hide();
		current_save = $(this).attr("which");
		$($(this).attr("which")).show();
		current_tab = $(this).attr("which");
		$('li.browser').removeClass('active');
		$(this).addClass('active');
		
	});
	$('select').each(function(){
		if($(this).attr("selected")){
			$(this).val($(this).attr("selected"));
		}
	})
	$('button.primaryAction').unbind("click");
	$('.datepick').datepicker();
	
	$('div.tabber').click(function(){
		$('.tab_content').hide();
		$($(this).attr("show")).show();
		$('.tabactive').removeClass("tabactive").addClass("tabinactive");
		$(this).removeClass("tabinactive").addClass("tabactive");
		$('#calendar').fullCalendar('render');
		
	});
	$('#deal_type_select').change(function(){
		$('.deal_specs').hide();
		$('.'+$(this).val()).show();
	})
	
	$( "#slider" ).slider({
				value:30,
				min: 7,
				max: 365,
				range: "min",
				step: 1,
				slide: function( event, ui ) {
					$( "#a_days" ).html( ui.value );
				}
			});
			$( "#a_days" ).html( $( "#slider" ).slider( "value" ) );
			
			
	$('#calendar').fullCalendar({
		header: {
			left: 'prev,next today',
			center: 'title',
			right: 'month,agendaWeek,agendaDay'
		},
		editable: true});
	

	
})
var current_tab = "#info";
	function switchTab(which){
		$(current_tab).hide();
		$(which).show();
		current_tab = which;
	}
    </script>
